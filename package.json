{
  "name": "schematics-scss-migrate",
  "version": "1.3.15",
  "description": "A schematic to migrate from CSS to SCSS stylesheet format for an Angular CLI project",
  "scripts": {
    "build-dev": "tsc -p tsconfig.json --watch",
    "build": "tsc -p tsconfig.json",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics",
    "angular-cli",
    "scss",
    "css",
    "styles",
    "stylesheet"
  ],
  "author": "Thabo Ambrose <ngubanethabo.ambrose@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/Teebo/scss-migrate.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Teebo/scss-migrate/issues"
  },
  "homepage": "https://github.com/Teebo/scss-migrate#readme",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^12.0.1",
    "@angular-devkit/schematics": "^12.2.8",
    "@schematics/angular": "^12.2.8",
    "glob": "^7.1.6",
    "sass": "^1.29.0",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "@types/sass": "^1.16.0",
    "jasmine": "^3.5.0"
  },
  "ng-add": {
    "save": "devDependencies"
  }
